{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "IdentityHub": "Information"
    }
  },
  "AllowedHosts": "*",
  "EntraId": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "<your-tenant-id>",
    "ClientId": "<your-client-id>",
    "ClientSecret": "",
    "Audience": "api://<your-client-id>",
    "UseManagedIdentity": false,
    "GraphApiScopes": ["https://graph.microsoft.com/.default"]
  },
  "RedisCache": {
    "Enabled": false,
    "ConnectionString": "localhost:6379",
    "DefaultExpirationSeconds": 300,
    "UserPermissionsExpirationSeconds": 300,
    "RolePermissionsExpirationSeconds": 3600,
    "GraphDataExpirationSeconds": 600
  }
}
